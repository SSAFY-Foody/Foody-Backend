<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
 <mapper namespace="com.ssafy.foody.character.mapper.CharacterMapper">
 	<select id = "findAllCharacters" resultType="FoodyCharacter">
 		select id, name, img_url as img, description,
 		ai_learning_comment as aiLearningComment
 		from characters
 	</select>
 </mapper>